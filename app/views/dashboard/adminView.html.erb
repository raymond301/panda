<h1>Admin Dashboard</h1>

<%if !@users.blank?%>
    User Stats:
     <table class="table">
         <tr><th>User</th><th>Email</th><th>Group</th><th># Annot</th><th># Logins</th><th>Last Login</th></tr>
        <%@users.each do |u|%>
         <tr><td><%=link_to u.username, :action=>'su', :id=>u.id%></td><td><%=u.email%></td><td><%=u.current_group_id%></td><td><%=u.my_annotations_or_group_annotaitons.size%></td><td><%=u.sign_in_count%></td><td><%=u.last_sign_in_at%></td></tr>
         <%end%>
     </table>
<%end%>

</br></br>

Pathway Stats:
<table class="table" style="width: 350px;">
    <tr><th>Source</th><th>Count</th></tr>
    <%@pathStats.each do |u|%>
        <tr><td><%=u.source%></td><td><%=u.cnt%></td></tr>
    <%end%>
</table>