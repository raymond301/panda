<h1>Upload a New Annotation</h1>

<%#raise flash.now[:error].inspect%>

 </br>
<%= form_tag({:controller=>'annotation_collections', :action=>'new_w_file'}, {:multipart => true, :class=>'form-horizontal'}) do %>
<div class="row">
    <div class="col-sm-5">
    <%= file_field_tag 'file', :class => 'form-control'%>
    </div>
    <div class="col-sm-1">
    <%= submit_tag "Upload", :class=>'btn btn-info btn-sm pull-right'%>
        </div>
    </div>
<% end %>

<%#= render 'form' %>

<%#= link_to 'Back', annotation_collections_path %>
